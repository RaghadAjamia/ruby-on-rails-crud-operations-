
<div class="container">
  <div class="row justify-content-center">
    <h4 class="mt-4 mb-4 display-4">Show details of location <%= @location.id %></h4>
  </div>
  <div class="row justify-content-center">
    <table class="table">
      <tbody>
        <tr>
          <th>Title:</th>
          <td><%= @location.address %></td>
        </tr>
        
      </tbody>
    </table>
  </div>
  <%= link_to 'Back ', locations_path, class: 'btn btn-primary mt-2' %>

</div>


